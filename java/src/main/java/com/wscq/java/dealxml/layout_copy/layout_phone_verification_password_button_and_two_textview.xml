<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/x395"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_rect_35_black_10dp_stroke_black"
    android:orientation="vertical"
    tools:showIn="@layout/activity_register">

    <include layout="@layout/view_phone_and_verify_code" />

    <include layout="@layout/layout_password_edit_text" />

    <TextView
        android:id="@+id/submit_button"
        style="@style/btn_rect_b1_1_pressed_b1_3_disable_b1_2_global"
        android:layout_marginTop="@dimen/y30"
        android:enabled="false"
        android:text="注册" />

    <include layout="@layout/layout_login_bottom_textview" />
</LinearLayout>
